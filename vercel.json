{
  "buildCommand": "npm run build",
  "outputDirectory": "dist",
  "rewrites": [
    {
      "source": "/landing",
      "destination": "/landing.html"
    },
    {
      "source": "/files",
      "destination": "/files.html"
    },
    {
      "source": "/search",
      "destination": "/search.html"
    },
    {
      "source": "/",
      "destination": "/index.html"
    }
  ]
}
